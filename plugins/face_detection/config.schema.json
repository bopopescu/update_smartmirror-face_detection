{
    "schema": {
        "userData": {
            "type": "array",
            "title": "userDatas Settings",
            "items": {
                "type": "object",
                    "properties": {
                       "userName":{
                                "type":"string",
                                "title": "your name"
                       },
                      "email": {
                          "type": "object",
                          "properties": {
                              "id": {
                                  "type": "string",
                                  "title": "your email address"
                              },
                              "password": {
                                  "type": "string",
                                  "title": "your email password or your email app password"
                              }
                          }
                      },
                      "calendar": {
                        "type": "object",
                        "title": "Calendar Settings",
                        "properties": {
                            "icals": {
                                "type": "array",
                                "title": "iCal URLs",
                                "items": {
                                    "type": "string"
                                }
                            },
                            "maxResults": {
                                "type": "integer",
                                "title": "Max Number of Events for all iCals",
                                "default": 9
                            },
                            "maxDays": {
                                "type": "integer",
                                "title": "Max Number of Days to display",
                                "default": 9
                            },
                            "showCalendarNames": {
                                "type": "boolean",
                                "title": "Check to display Calendar Names",
                                "default": false
                             }
                        }
                      }
                    }
            }
        }
    },
    "form":[
        {
          "type":"fieldset",
          "expandable":true,
          "title":"User Settings",
          "order":0,

              "items": [
                {
                  "type":"tabarray",
                  "items": [
                    {
                      "type":"section",
                      "legend":"{{value}}",
                      "items":[
                          {
                            "key":"userData[].userName",
                            "valueInLegend":true
                          },
                          "userData[].email.id",
                          "userData[].email.password",
                          {
                            "type":"tabarray",
                            "title":"calendars",
                            "items":[
                              {

                                      "type":"section",
                                      "legend":"iCal {{idx}}",
                                      "items":[
                                          {
                                          "key":"userData[].calendar.icals[]",
                                          "type":"textarea"
                                          }
                                      ]



                              }

                      ]
                    }
                  ]
                }
              ]
            }
          ]
        }
      ],
